{
  "name": "@react-synth/synth",
  "version": "0.0.6-alpha",
  "description": "Live coding music with React",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./samples/*": "./src/samples/*"
  },
  "files": [
    "dist",
    "src/samples"
  ],
  "bin": {
    "react-synth": "./dist/cli.js"
  },
  "scripts": {
    "build": "vite build",
    "dev": "tsx cli/cli.ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "chokidar": "^4.0.3",
    "esbuild": "^0.27.2",
    "jsdom": "^25.0.1",
    "node-web-audio-api": "^1.0.7",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "tonal": "^6.4.2"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vite": "^6.0.7",
    "vite-plugin-dts": "^4.4.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "react",
    "music",
    "audio",
    "synthesizer",
    "live-coding",
    "web-audio"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/react-synth.git"
  }
}
