{
  "name": "@react-synth/synth",
  "version": "0.0.6-alpha",
  "license": "MIT",
  "exports": {
    ".": "./src/index.ts",
    "./samples/bd_haus.flac": "./src/samples/bd_haus.flac",
    "./samples/drum_cymbal_closed.flac": "./src/samples/drum_cymbal_closed.flac"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "tasks": {
    "dev": "deno run --watch --allow-all examples/simple/simple.tsx",
    "test": "deno run --allow-ffi --allow-read --watch examples/test.ts"
  },
  "nodeModulesDir": "auto",
  "imports": {
    "@react-synth/synth": "./src/index.ts",
    "react": "npm:react@19",
    "@types/react": "npm:@types/react@19",
    "react-dom": "npm:react-dom@19",
    "react-dom/client": "npm:react-dom@19/client",
    "react/jsx-runtime": "npm:react@19/jsx-runtime",
    "jsdom": "npm:jsdom@24",
    "node-web-audio-api": "npm:node-web-audio-api@^1.0.7",
    "tonal": "npm:tonal@^6.4.2"
  }
}
