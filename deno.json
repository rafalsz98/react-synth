{
  "name": "@react-synth/synth",
  "version": "0.0.4-alpha",
  "license": "MIT",
  "exports": {
    ".": "./src/components/index.ts",
    "./cli": "./cli/cli.ts"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "tasks": {
    "dev": "deno run --allow-read --allow-env --allow-sys --allow-ffi cli/cli.ts examples/simple.tsx",
    "play": "deno run --allow-read --allow-env --allow-sys --allow-ffi cli/cli.ts",
    "test": "deno run --allow-ffi --allow-read --watch examples/test.ts"
  },
  "nodeModulesDir": "auto",
  "imports": {
    "@react-synth/synth": "./src/components/index.ts",
    "@std/path": "jsr:@std/path@1",
    "react": "npm:react@19",
    "@types/react": "npm:@types/react@19",
    "react-dom": "npm:react-dom@19",
    "react-dom/client": "npm:react-dom@19/client",
    "react/jsx-runtime": "npm:react@19/jsx-runtime",
    "jsdom": "npm:jsdom@24",
    "node-web-audio-api": "npm:node-web-audio-api@^1.0.7",
    "tonal": "npm:tonal@^6.4.2"
  }
}
