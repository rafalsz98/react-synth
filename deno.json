{
  "name": "@synth/synth",
  "version": "0.1.0",
  "exports": "./components/index.ts",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "synth"
  },
  "tasks": {
    "dev": "deno run --allow-ffi --allow-read --allow-env --unstable-ffi --watch=examples/simple.tsx bin/runner.ts examples/simple.tsx",
    "example": "deno run --allow-ffi --allow-read --allow-env --unstable-ffi bin/runner.ts examples/simple.tsx",
    "build:rust": "cd audio_engine && cargo build --release"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "@std/path": "https://deno.land/std@0.224.0/path/mod.ts",
    "synth": "./components/index.ts",
    "synth/jsx-runtime": "./components/jsx-runtime.ts",
    "synth/": "./components/"
  }
}
